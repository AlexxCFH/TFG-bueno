{
  "name": "@fynnix/node-easy-ipc",
  "version": "1.1.4",
  "description": "Easy IPC addon implementation for Windows using messages, filemapping, mutex, threads.",
  "main": "build/Release/node_easy_ipc.node",
  "scripts": {
    "install": "npm run addon:build",
    "addon:build": "npx node-gyp rebuild",
    "addon:clean": "npx node-gyp clean",
    "test:electron": "npx electron-rebuild --version 33.0.0 --debug --module-dir . -p"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FynniX/node-easy-ipc.git"
  },
  "keywords": [
    "windows",
    "ipc",
    "getmessage",
    "sendmessage",
    "mutex",
    "thread",
    "filemapping"
  ],
  "author": "Winexcel",
  "license": "ISC",
  "gypfile": true,
  "bugs": {
    "url": "https://github.com/FynniX/node-easy-ipc/issues"
  },
  "homepage": "https://github.com/FynniX/node-easy-ipc#readme",
  "dependencies": {
    "node-addon-api": "^8.2.1"
  }
}
